{
  "manifest_version": 3,
  "name": "Traffic Sniffer",
  "version": "1.1",
  "description": "Logs browsing activity, associated tab, and timestamps.",
  "permissions": [
    "webRequest",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Logs"
  }
  ,
  "host_permissions": [
    "<all_urls>"
  ]
}
